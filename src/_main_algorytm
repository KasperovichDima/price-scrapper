"""Main programm algorytm schema."""
from typing import Iterable, List
import project_typing  as pt
from classes import interfaces as i


def get_parsers(shops: List[int]) -> Iterable[i.IParser]:
    """Create and return parser instances."""

def get_report(request: pt.Request) -> i.IReport:
    report = Report(request.user)
    parsers = get_parsers(request.shops)
    for parser in parsers:
        parser.add_prices(report)
    return report


user: i.IUser
elements: List[int]
shops: List[int]
request = pt.Request(user, elements, shops)

report = get_report(request)
report.save()
report.view()
